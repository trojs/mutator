{
    "name": "@trojs/mutator",
    "description": "Mutate the value when set some data",
    "version": "1.0.4",
    "author": {
        "name": "Pieter Wigboldus",
        "url": "https://trojs.org/"
    },
    "license": "MIT",
    "scripts": {
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "lint:report": "eslint src/*.js -f json -o report.json",
        "test": "node --test --experimental-test-coverage --test-reporter=spec --test-reporter=lcov --test-reporter-destination=stdout --test-reporter-destination=./coverage/lcov.info",
        "cpd": "node_modules/jscpd/bin/jscpd src",
        "vulnerabilities": "npm audit --only=prod"
    },
    "type": "module",
    "files": [
        "src/default.js",
        "src/string-helper.js"
    ],
    "main": "src/default.js",
    "devDependencies": {
        "@trojs/lint": "^0.2.9",
        "eslint": "^9.15.0",
        "globals": "^16.0.0",
        "jscpd": "^4.0.5"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/trojs/mutator"
    },
    "engines": {
        "node": ">= 20"
    },
    "keywords": [
        "object",
        "mutate"
    ]
